{
  "name": "drawsome",
  "version": "0.0.1",
  "description": "A happy little skeleton.",
  "main": "index.js",
  "scripts": {
    "test": "check-node-version --node '>= 6.7.0' && bin/setup && mocha --reporter nyan --compilers js:babel-register $(find . -name '*.spec.js*' ! -path '*node_modules*')",
    "test-watch": "check-node-version --node '>= 6.7.0' && bin/setup && mocha --compilers js:babel-register --watch app/**/*.test.js app/**/*.test.jsx db/**/*.test.js server/**/*.test.js",
    "build": "check-node-version --node '>= 6.7.0' && bin/setup && webpack",
    "build-watch": "check-node-version --node '>= 6.7.0' && bin/setup && webpack -w",
    "build-css": "node-sass -w assets/stylesheets/style.scss -o public/stylesheets",
    "build-branch": "bin/build-branch.sh",
    "deploy-heroku": "bin/deploy-heroku.sh",
    "start": "check-node-version --node '>= 6.7.0' && bin/setup && nodemon -x 'node --harmony' server/start.js",
    "seed": "node --harmony db/seed.js",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/queerviolet/bones.git"
  },
  "engines": {
    "node": "7.5.0"
  },
  "keywords": [
    "react",
    "redux",
    "skeleton"
  ],
  "author": "Ashi Krishnan <me@ashi.works>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/queerviolet/bones/issues"
  },
  "homepage": "https://github.com/queerviolet/bones#readme",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-preset-stage-2": "^6.18.0",
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.15.2",
    "bootstrap-social": "^5.1.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "chalk": "^1.1.3",
    "check-node-version": "^1.1.2",
    "cookie-session": "^2.0.0-alpha.1",
    "enzyme": "^2.7.1",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "jsdom": "^9.9.1",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "node-gyp": "^3.5.0",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "optimist": "^0.6.1",
    "paper": "^0.10.2",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.10",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^6.1.0",
    "pretty-error": "^2.0.2",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "rebuild": "^0.1.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "sequelize": "^3.24.6",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "socket.io": "^1.7.2",
    "babel": "^6.5.2",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "socket.io-client": "^1.7.2",
    "volleyball": "^1.4.1",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.1.2",
    "redux-mock-store": "^1.2.2",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.1"
  }
}
